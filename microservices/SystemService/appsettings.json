{
  "AuthServer": {
    "Authority": "https://localhost:3000",
    "RequireHttpsMetadata": "true",
    "ApiName": "SystemService"
  },
  "ConnectionStrings": {
    "Default": "Server=localhost,1433; Database=Micro_Identity; User=sa; Password=yourStrong(!)Password",
  },
  "Redis": {
    "Configuration": "localhost:6379"
  },
  "ElasticSearch": {
    "Url": "http://localhost:9200"
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "localhost"
      }
    },
    "EventBus": {
      "ClientName": "Micro_BasicService",
      "ExchangeName": "Micro"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
